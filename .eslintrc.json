{
    "env": {
        "browser": true,
        "es2021": true,
        "cypress/globals": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react", "import", "cypress"
    ],
    "rules": {
        "max-len": ["off", { "code": 200 }],
        "require-jsdoc": ["off"],
        "no-param-reassign": ["error", { "props": false }]
    }
}
